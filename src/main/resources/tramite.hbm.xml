<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="domain.Tramite" table="tramite">
        <id name="idTramite" type="int" column="idTramite">
            <generator class="native"/>
        </id>
        <property name="tipoTramite" column="tipoTramite" type="string"/>
        <property name="fechaTramite" column="fechaTramite" type="timestamp"/>
    </class>
</hibernate-mapping>